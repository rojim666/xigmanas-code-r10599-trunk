msgid "Enable Selected NFS Exports"
msgstr "ເປີດໃຊ້ງານການສົ່ງອອກ NFS ທີ່ເລືອກ"

msgid "Toggle Selected NFS Exports"
msgstr "ສະຫຼັບການໃຊ້ງານການສົ່ງອອກ NFS ທີ່ເລືອກ"

msgid "Do you want to delete selected NFS exports?"
msgstr "ທ່ານຕ້ອງການລຶບການສົ່ງອອກ NFS ທີ່ເລືອກອອກບໍ່?"

msgid "Do you want to disable selected NFS exports?"
msgstr "ທ່ານຕ້ອງການປິດໃຊ້ງານການສົ່ງອອກ NFS ທີ່ເລືອກບໍ່?"

msgid "Do you want to enable selected NFS exports?"
msgstr "ທ່ານຕ້ອງການເປີດໃຊ້ງານການສົ່ງອອກ NFS ທີ່ເລືອກບໍ່?"

msgid "Do you want to toggle selected NFS exports?"
msgstr "ທ່ານຕ້ອງການສະຫຼັບການໃຊ້ງານການສົ່ງອອກ NFS ທີ່ເລືອກບໍ່?"

msgid "Mapping"
msgstr "ການຈັບຄູ່"

msgid "Map All To User"
msgstr "ຈັບຄູ່ທັງໝົດກັບຜູ້ໃຊ້"

msgid "Map Root To User"
msgstr "ຈັບຄູ່ Root ກັບຜູ້ໃຊ້"

msgid "Export Subfolders"
msgstr "ສົ່ງອອກໂຟນເດີຍ່ອຍ"

msgid "WebNFS"
msgstr "WebNFS"

msgid "Be Quiet"
msgstr "ແບບງຽບ"

msgid "Security Flavor"
msgstr "ປະເພດຄວາມປອດໄພ"

msgid "Root Directory"
msgstr "ໂຟນເດີຫຼັກ"

msgid "Map users or map root to the specified user."
msgstr "ຈັບຄູ່ຜູ້ໃຊ້ ຫຼື ຈັບຄູ່ root ກັບຜູ້ໃຊ້ທີ່ລະບຸ."

msgid "mapall - Specifies a mapping for all client UIDs (including root)"
msgstr "mapall - ລະບຸການຈັບຄູ່ສຳລັບ UID ຂອງ client ທັງໝົດ (ລວມທັງ root)"

msgid ""
"maproot - The credential of the specified user is used for remote access by "
"root."
msgstr ""
"maproot - ຂໍ້ມູນປະຈຳຕົວຂອງຜູ້ໃຊ້ທີ່ລະບຸຈະຖືກໃຊ້ສຳລັບການເຂົ້າເຖິງຈາກທາງໄກໂດຍ "
"root."

msgid ""
"Remote accesses by root will result in using a credential of 65534:65533. "
"All other users will be mapped to their remote credential."
msgstr ""
"ການເຂົ້າເຖິງຈາກທາງໄກໂດຍ root ຈະສົ່ງຜົນໃຫ້ໃຊ້ຂໍ້ມູນປະຈຳຕົວຂອງ 65534:65533. "
"ຜູ້ໃຊ້ອື່ນໆທັງໝົດຈະຖືກຈັບຄູ່ກັບຂໍ້ມູນປະຈຳຕົວທາງໄກຂອງພວກເຂົາ."

msgid "Enter the network that is authorised to access the NFS share."
msgstr "ປ້ອນເຄືອຂ່າຍທີ່ໄດ້ຮັບອະນຸຍາດໃຫ້ເຂົ້າເຖິງການແບ່ງປັນ NFS."

msgid "192.168.0.0/24"
msgstr "192.168.0.0/24"

msgid "Export all the directories in the specified path."
msgstr "ສົ່ງອອກໂຟນເດີທັງໝົດໃນເສັ້ນທາງທີ່ລະບຸ."

msgid "To use subdirectories, you must mount each directory."
msgstr "ເພື່ອໃຊ້ໂຟນເດີຍ່ອຍ, ທ່ານຕ້ອງ mount ແຕ່ລະໂຟນເດີ."

msgid "Export as WebNFS."
msgstr "ສົ່ງອອກເປັນ WebNFS."

msgid "Note that only one file system can be WebNFS exported on a server."
msgstr ""
"ໝາຍເຫດ: ມີພຽງລະບົບໄຟລ໌ດຽວເທົ່ານັ້ນທີ່ສາມາດສົ່ງອອກເປັນ WebNFS ໄດ້ໃນເຊີບເວີ."

msgid "Inhibit some of the syslog diagnostics for bad lines in /etc/exports."
msgstr ""
"ຢັບຢັ້ງການວິນິດໄສ syslog ບາງຢ່າງສຳລັບບັນທັດທີ່ບໍ່ຖືກຕ້ອງໃນ /etc/exports."

msgid "Export the file system in read-only mode."
msgstr "ສົ່ງອອກລະບົບໄຟລ໌ໃນໂໝດອ່ານຢ່າງດຽວ."

msgid ""
"Specify a list of acceptable security flavors to be used for remote access. "
"The\tdefault\tsecurity flavor\tis sys."
msgstr ""
"ລະບຸລາຍຊື່ຂອງປະເພດຄວາມປອດໄພທີ່ຍອມຮັບໄດ້ເພື່ອໃຊ້ສຳລັບການເຂົ້າເຖິງຈາກທາງໄກ. "
"ປະເພດຄວາມປອດໄພເລີ່ມຕົ້ນແມ່ນ sys."

msgid "sys - Use AUTH_SYS authentication."
msgstr "sys - ໃຊ້ການຢືນຢັນຕົວຕົນ AUTH_SYS."

msgid ""
"krb5 - Use Kerberos V5 protocol to authenticate users before granting access "
"to the shared filesystem."
msgstr ""
"krb5 - ໃຊ້ໂປຣໂຕຄໍ Kerberos V5 "
"ເພື່ອຢືນຢັນຕົວຕົນຜູ້ໃຊ້ກ່ອນທີ່ຈະອະນຸຍາດໃຫ້ເຂົ້າເຖິງລະບົບໄຟລ໌ທີ່ແບ່ງປັນ."

msgid "krb5i - Use Kerberos V5 authentication with data integrity checksums."
msgstr ""
"krb5i - ໃຊ້ການຢືນຢັນຕົວຕົນ Kerberos V5 ພ້ອມກັບ checksum ຄວາມສົມບູນຂອງຂໍ້ມູນ."

msgid ""
"krb5p - Use Kerberos V5 authentication with data integrity checksums and "
"data encryption."
msgstr ""
"krb5p - ໃຊ້ການຢືນຢັນຕົວຕົນ Kerberos V5 ພ້ອມກັບ checksum ຄວາມສົມບູນຂອງຂໍ້ມູນ "
"ແລະ ການເຂົ້າລະຫັດຂໍ້ມູນ."

msgid "Root Directories"
msgstr "ໂຟນເດີຫຼັກ"

msgid "Add NFS Root"
msgstr "ເພີ່ມ NFS Root"

msgid "Edit NFS Root"
msgstr "ແກ້ໄຂ NFS Root"

msgid "NFS Root is marked for deletion"
msgstr "NFS Root ຖືກໝາຍໄວ້ເພື່ອລຶບ"

